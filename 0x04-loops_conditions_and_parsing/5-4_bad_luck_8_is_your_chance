#!/usr/bin/env bash
# This script is displaying "a software development program" 10 times, plus 'good luck'
# and 'bad luck'
i=0
while [ $i -lt 10 ]; do

	if (( i == 3 )); then
		echo "bad luck"
	elif (( i == 7 )); then
		echo "good luck"
	else
		echo "a software development program"
	fi

	(( i++ ))

done
